{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "Martin, Jaffre, Roberts: test 1, constant K on fracture.", 
    mesh = {
      mesh_file = "mesh_2d1d.msh",
      sets = [
          {name = "matrix",
           region_labels = [ "hornina"]
          }
      ]
    }, 

    primary_equation = {
      TYPE = "Steady_MH", 

      input_fields= [
        { r_set = "matrix",
          conductivity = 1,            
          cross_section = 1
        },
        { region = "puklina",
          conductivity = 100,
          cross_section = $rozevreni$
        },
        { r_set = "BOUNDARY",
          bc_type = "dirichlet",
          bc_pressure = {TYPE = "FieldFormula", 
                         value = "y+x+if(x>0,1,-1)*$rozevreni$/2"}
        }
      ],

      output = {
        output_stream = {
          file = "flow.pvd", 
            format = {
            TYPE = "vtk", 
            variant = "ascii"
          }
        },
        output_fields = [ "pressure_p0", "velocity_p0", "region_id"]
      },
      balance = {
        balance_output="water_balance_2d1d.out"
      },
          
      solver = {
        TYPE = "Petsc", 
        a_tol = 1e-12
        r_tol = 1e-12
      } 
    }
  } // problem
}